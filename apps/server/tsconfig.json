{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "CommonJS",
    "lib": ["ES2020"],
    "outDir": "dist",
    "rootDir": "src",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": { "src/*": ["src/*"], "generated/*": ["generated/*"] }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
